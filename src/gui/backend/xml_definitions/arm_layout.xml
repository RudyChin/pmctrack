<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE layout SYSTEM "layout.dtd">
<layout vendor="arm"  version="0.0">
	<!-- number of bits for each configurable field -->

  <field>
    <name>os</name>
    <nbits>1</nbits>
    <default>0</default>
  </field>			
  <field>
    <name>usr</name>
    <nbits>1</nbits>
    <default>1</default>
  </field>	
  <field>
    <name>ebs</name>
    <nbits>32</nbits>
    <default>0</default>
  </field>	
  
</layout>
